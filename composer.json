{
    "name": "lightster/hodor",
    "description": "PHP/rabbitmq-based job queue",
    "keywords": [
        "hodor",
        "php",
        "rabbitmq",
        "job",
        "queue"
    ],
    "homepage": "http://github.com/lightster/hodor",
    "autoload": {
        "psr-0": {
            "Hodor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "Hodor\\": "tests/src/"
        }
    },
    "require": {
        "davedevelopment/phpmig": "^1.1.0",
        "lightster/yo-pdo": "^0.1.3",
        "php": ">=5.5.18",
        "pimple/pimple": "^1.1.1",
        "php-amqplib/php-amqplib": "^2.4.0",
        "symfony/console": "^2.8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.0",
        "mikey179/vfsStream": "^1.6.4",
        "codeclimate/php-test-reporter": "dev-master"
    }
}
